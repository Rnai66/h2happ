{
  "info": {
    "name": "H2H v0.1.3",
    "_postman_id": "h2h-013-collection",
    "description": "Auth + CRUD basic endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health",
      "request": {
        "method": "GET",
        "url": "http://localhost:4000/api/health"
      }
    },
    {
      "name": "Auth - Register",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\"name\": \"Rnai\", \"email\": \"rnai@example.com\", \"password\": \"123456\"}"
        },
        "url": "http://localhost:4000/api/auth/register"
      }
    },
    {
      "name": "Auth - Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\"email\": \"rnai@example.com\", \"password\": \"123456\"}"
        },
        "url": "http://localhost:4000/api/auth/login"
      }
    },
    {
      "name": "Auth - Profile",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "url": "http://localhost:4000/api/auth/profile"
      }
    },
    {
      "name": "Users - List",
      "request": {
        "method": "GET",
        "url": "http://localhost:4000/api/users"
      }
    },
    {
      "name": "Users - Create",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\"name\": \"Alice\", \"email\": \"alice@example.com\", \"password\": \"pass\"}"
        },
        "url": "http://localhost:4000/api/users"
      }
    }
  ],
  "variable": [
    {
      "key": "token",
      "value": ""
    }
  ]
}